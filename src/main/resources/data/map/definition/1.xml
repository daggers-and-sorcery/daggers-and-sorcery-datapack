<?xml version="1.0" encoding="UTF-8"?>

<map:map xmlns:map="http://www.swordsandsorcery.com/map" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.swordsandsorcery.com/map ../schema.xsd">
    <map:id>1</map:id>
    <map:main-shop>1</map:main-shop>
    <map:spawn>
        <map:timer>5</map:timer>
        <!-- respawn in seconds -->
        <map:alive>4000</map:alive>
        <!-- 100 npc can be alive at one time on the map -->
        <map:spawns>
            <map:monster>
                <map:id>1</map:id>
                <map:chance>50</map:chance>
            </map:monster>
            <map:monster>
                <map:id>2</map:id>
                <map:chance>20</map:chance>
            </map:monster>
            <map:monster>
                <map:id>3</map:id>
                <map:chance>15</map:chance>
            </map:monster>
            <map:monster>
                <map:id>4</map:id>
                <map:chance>10</map:chance>
            </map:monster>
            <map:monster>
                <map:id>5</map:id>
                <map:chance>5</map:chance>
            </map:monster>
        </map:spawns>
    </map:spawn>
    <map:objects>
        <map:object x="111" y="22">
            <map:name>Mapple tree</map:name>
            <map:type>TREE</map:type>
        </map:object>
        <map:object x="11" y="22">
            <map:name>Oak tree</map:name>
            <map:type>TREE</map:type>
        </map:object>
    </map:objects>
</map:map>